{
  "name": "hospitals",
  "version": "1.0.0",
  "description": "API to get list of hospitals in Nigeria",
  "main": "server.js",
  "scripts": {
    "build": "babel ./src -d ./build",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start:dev": "nodemon --exec babel-node src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate:fresh": "sequelize db:migrate && npm run db:seed",
    "db:migrate": "sequelize db:migrate && npm run db:seed",
    "db:seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danprocoder/hospitals-api.git"
  },
  "keywords": [
    "hospitals"
  ],
  "author": "Daniel Austin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danprocoder/hospitals-api/issues"
  },
  "homepage": "https://github.com/danprocoder/hospitals-api#readme",
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.4"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:check"
    }
  }
}
